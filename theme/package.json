{
  "name": "gatsby-theme-rocket",
  "description": ":rocket: A super fast blog",
  "version": "1.1.0",
  "author": "Raincal <cyj94228@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/Raincal/gatsby-theme-rocket"
  },
  "keywords": [
    "gatsby",
    "gatsby-theme",
    "gatsby-plugin",
    "blog"
  ],
  "license": "MIT",
  "scripts": {
    "format:code": "npm run lint:code -- --fix",
    "lint:code": "eslint --ignore-path .gitignore --ext .js,.jsx .",
    "lint:md": "remark src/pages/",
    "format": "prettier --write 'src/**/*.{js,jsx}'"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.5.8",
    "@mdx-js/react": "^1.5.8",
    "dayjs": "^1.8.23",
    "gatsby-paginate": "^1.1.1",
    "gatsby-plugin-brotli": "^1.3.1",
    "gatsby-plugin-catch-links": "^2.1.28",
    "gatsby-plugin-feed": "^2.3.29",
    "gatsby-plugin-layout": "^1.1.24",
    "gatsby-plugin-mdx": "^1.0.83",
    "gatsby-plugin-netlify": "^2.1.35",
    "gatsby-plugin-no-sourcemaps": "^2.1.2",
    "gatsby-plugin-page-creator": "^2.1.46",
    "gatsby-plugin-postcss": "^2.1.22",
    "gatsby-plugin-react-helmet": "^3.1.24",
    "gatsby-plugin-sharp": "^2.4.13",
    "gatsby-plugin-sitemap": "^2.2.30",
    "gatsby-remark-autolink-headers": "^2.1.26",
    "gatsby-remark-images": "^3.1.50",
    "gatsby-remark-prismjs": "^3.3.36",
    "gatsby-remark-yuque-images": "^1.2.0",
    "gatsby-source-filesystem": "^2.1.56",
    "gatsby-transformer-remark": "^2.6.59",
    "gatsby-transformer-yaml": "^2.2.27",
    "lodash": "^4.17.15",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "prismjs": "^1.19.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "gatsby": "^2.19.45",
    "prettier": "^1.19.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "remark-cli": "^7.0.1",
    "remark-preset-lint-recommended": "^3.0.3"
  },
  "peerDependencies": {
    "gatsby": "^2.19.45",
    "react": "^16.13.0",
    "react-dom": "^16.13.0"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended",
      [
        "lint-no-shortcut-reference-link",
        false
      ],
      [
        "lint-no-undefined-references",
        false
      ]
    ]
  }
}
