{
  "name": "gatsby-theme-rocket",
  "description": ":rocket: A super fast blog",
  "version": "1.1.0",
  "author": "Raincal <cyj94228@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/Raincal/gatsby-theme-rocket"
  },
  "keywords": [
    "gatsby",
    "gatsby-theme",
    "gatsby-plugin",
    "blog"
  ],
  "license": "MIT",
  "scripts": {
    "format:code": "npm run lint:code -- --fix",
    "lint:code": "eslint --ignore-path .gitignore --ext .js,.jsx .",
    "lint:md": "remark src/pages/",
    "format": "prettier --write 'src/**/*.{js,jsx}'"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.4.0",
    "@mdx-js/react": "^1.4.0",
    "dayjs": "^1.8.16",
    "gatsby-paginate": "^1.1.1",
    "gatsby-plugin-brotli": "^1.2.4",
    "gatsby-plugin-catch-links": "^2.1.6",
    "gatsby-plugin-feed": "^2.3.9",
    "gatsby-plugin-layout": "^1.1.5",
    "gatsby-plugin-mdx": "^1.0.33",
    "gatsby-plugin-netlify": "^2.1.10",
    "gatsby-plugin-no-sourcemaps": "^2.1.1",
    "gatsby-plugin-page-creator": "^2.1.14",
    "gatsby-plugin-postcss": "^2.1.5",
    "gatsby-plugin-react-helmet": "^3.1.5",
    "gatsby-plugin-sharp": "^2.2.18",
    "gatsby-plugin-sitemap": "^2.2.9",
    "gatsby-remark-autolink-headers": "^2.1.8",
    "gatsby-remark-images": "^3.1.19",
    "gatsby-remark-prismjs": "^3.3.11",
    "gatsby-remark-yuque-images": "^1.0.0-beta.0",
    "gatsby-source-filesystem": "^2.1.18",
    "gatsby-transformer-remark": "^2.6.19",
    "gatsby-transformer-yaml": "^2.2.7",
    "lodash": "^4.17.15",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "prismjs": "^1.17.1",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "eslint": "^6.2.2",
    "eslint-plugin-react": "^7.14.3",
    "gatsby": "^2.15.1",
    "prettier": "^1.18.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "remark-cli": "^7.0.0",
    "remark-preset-lint-recommended": "^3.0.3"
  },
  "peerDependencies": {
    "gatsby": "^2.13.45",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended",
      [
        "lint-no-shortcut-reference-link",
        false
      ],
      [
        "lint-no-undefined-references",
        false
      ]
    ]
  }
}
