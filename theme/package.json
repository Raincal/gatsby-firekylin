{
  "name": "gatsby-theme-rocket",
  "description": ":rocket: A super fast blog",
  "version": "1.1.0",
  "author": "Raincal <cyj94228@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/Raincal/gatsby-theme-rocket"
  },
  "keywords": [
    "gatsby",
    "gatsby-theme",
    "gatsby-plugin",
    "blog"
  ],
  "license": "MIT",
  "scripts": {
    "format:code": "npm run lint:code -- --fix",
    "lint:code": "eslint --ignore-path .gitignore --ext .js,.jsx .",
    "lint:md": "remark src/pages/",
    "format": "prettier --write 'src/**/*.{js,jsx}'"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.5",
    "@mdx-js/react": "^1.6.5",
    "dayjs": "^1.8.28",
    "gatsby-paginate": "^1.1.1",
    "gatsby-plugin-brotli": "^1.3.1",
    "gatsby-plugin-catch-links": "^2.3.5",
    "gatsby-plugin-feed": "^2.5.5",
    "gatsby-plugin-layout": "^1.3.4",
    "gatsby-plugin-mdx": "^1.2.15",
    "gatsby-plugin-netlify": "^2.3.5",
    "gatsby-plugin-no-sourcemaps": "^2.2.0",
    "gatsby-plugin-page-creator": "^2.3.9",
    "gatsby-plugin-postcss": "^2.3.4",
    "gatsby-plugin-react-helmet": "^3.3.4",
    "gatsby-plugin-sharp": "^2.6.11",
    "gatsby-plugin-sitemap": "^2.4.5",
    "gatsby-remark-autolink-headers": "^2.3.5",
    "gatsby-remark-images": "^3.3.10",
    "gatsby-remark-prismjs": "^3.5.4",
    "gatsby-remark-yuque-images": "^1.2.0",
    "gatsby-source-filesystem": "^2.3.11",
    "gatsby-transformer-remark": "^2.8.15",
    "gatsby-transformer-yaml": "^2.4.4",
    "lodash": "^4.17.15",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "prismjs": "^1.20.0",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "eslint": "^7.2.0",
    "eslint-plugin-react": "^7.20.0",
    "gatsby": "^2.23.3",
    "prettier": "^2.0.5",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "remark-cli": "^8.0.0",
    "remark-preset-lint-recommended": "^4.0.0"
  },
  "peerDependencies": {
    "gatsby": "^2.23.3",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended",
      [
        "lint-no-shortcut-reference-link",
        false
      ],
      [
        "lint-no-undefined-references",
        false
      ]
    ]
  }
}
