{
  "name": "gatsby-theme-rocket",
  "description": ":rocket: A super fast blog",
  "version": "1.1.0",
  "author": "Raincal <cyj94228@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/Raincal/gatsby-theme-rocket"
  },
  "keywords": [
    "gatsby",
    "gatsby-theme",
    "gatsby-plugin",
    "blog"
  ],
  "license": "MIT",
  "scripts": {
    "format:code": "npm run lint:code -- --fix",
    "lint:code": "eslint --ignore-path .gitignore --ext .js,.jsx .",
    "lint:md": "remark src/pages/",
    "format": "prettier --write 'src/**/*.{js,jsx}'"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.5.1",
    "@mdx-js/react": "^1.5.1",
    "dayjs": "^1.8.16",
    "gatsby-paginate": "^1.1.1",
    "gatsby-plugin-brotli": "^1.3.0",
    "gatsby-plugin-catch-links": "^2.1.15",
    "gatsby-plugin-feed": "^2.3.19",
    "gatsby-plugin-layout": "^1.1.13",
    "gatsby-plugin-mdx": "^1.0.52",
    "gatsby-plugin-netlify": "^2.1.22",
    "gatsby-plugin-no-sourcemaps": "^2.1.1",
    "gatsby-plugin-page-creator": "^2.1.27",
    "gatsby-plugin-postcss": "^2.1.13",
    "gatsby-plugin-react-helmet": "^3.1.13",
    "gatsby-plugin-sharp": "^2.2.32",
    "gatsby-plugin-sitemap": "^2.2.19",
    "gatsby-remark-autolink-headers": "^2.1.16",
    "gatsby-remark-images": "^3.1.28",
    "gatsby-remark-prismjs": "^3.3.20",
    "gatsby-remark-yuque-images": "^1.0.0-beta.0",
    "gatsby-source-filesystem": "^2.1.33",
    "gatsby-transformer-remark": "^2.6.30",
    "gatsby-transformer-yaml": "^2.2.15",
    "lodash": "^4.17.15",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "prismjs": "^1.17.1",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-plugin-react": "^7.16.0",
    "gatsby": "^2.16.5",
    "prettier": "^1.18.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "remark-cli": "^7.0.0",
    "remark-preset-lint-recommended": "^3.0.3"
  },
  "peerDependencies": {
    "gatsby": "^2.13.45",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended",
      [
        "lint-no-shortcut-reference-link",
        false
      ],
      [
        "lint-no-undefined-references",
        false
      ]
    ]
  }
}
