{
  "name": "gatsby-rocket-blog",
  "description": ":rocket: A super fast blog",
  "version": "2.13.19",
  "author": "Raincal <cyj94228@gmail.com>",
  "dependencies": {
    "@mdx-js/mdx": "^1.1.0",
    "@mdx-js/react": "^1.0.27",
    "dayjs": "^1.8.15",
    "gatsby": "^2.13.39",
    "gatsby-paginate": "^1.0.17",
    "gatsby-plugin-brotli": "^1.2.3",
    "gatsby-plugin-catch-links": "^2.1.2",
    "gatsby-plugin-feed": "^2.3.5",
    "gatsby-plugin-layout": "^1.1.2",
    "gatsby-plugin-manifest": "^2.2.4",
    "gatsby-plugin-mdx": "^1.0.16",
    "gatsby-plugin-netlify": "^2.1.3",
    "gatsby-plugin-no-sourcemaps": "^2.1.1",
    "gatsby-plugin-offline": "^2.2.4",
    "gatsby-plugin-postcss": "^2.1.2",
    "gatsby-plugin-react-helmet": "^3.1.2",
    "gatsby-plugin-sharp": "^2.2.9",
    "gatsby-plugin-sitemap": "^2.2.3",
    "gatsby-remark-autolink-headers": "^2.1.3",
    "gatsby-remark-images": "^3.1.7",
    "gatsby-remark-prismjs": "^3.3.3",
    "gatsby-remark-yuque-images": "^0.3.0",
    "gatsby-source-filesystem": "^2.1.6",
    "gatsby-source-yuque": "^0.1.0",
    "gatsby-transformer-remark": "^2.6.9",
    "lodash": "^4.17.15",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "prismjs": "^1.17.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1"
  },
  "keywords": [
    "gatsby",
    "blog"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "build:prod": "yarn clean && yarn build",
    "deploy": "yarn build && ./deploy.sh",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "rm -rf .cache public",
    "format:code": "npm run lint:code -- --fix",
    "lint:code": "eslint --ignore-path .gitignore --ignore-pattern /functions --ext .js,.jsx .",
    "lint:md": "remark src/pages/",
    "format": "prettier --write 'src/**/*.{js,jsx}'"
  },
  "devDependencies": {
    "eslint": "^6.1.0",
    "eslint-plugin-react": "^7.14.3",
    "prettier": "^1.18.2",
    "remark-cli": "^7.0.0",
    "remark-preset-lint-recommended": "^3.0.3"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended",
      [
        "lint-no-shortcut-reference-link",
        false
      ],
      [
        "lint-no-undefined-references",
        false
      ]
    ]
  }
}
