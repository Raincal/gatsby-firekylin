{
  "name": "gatsby-rocket-blog",
  "description": ":rocket: A super fast blog",
  "version": "2.2.8",
  "author": "Raincal <cyj94228@gmail.com>",
  "dependencies": {
    "@mdx-js/mdx": "^0.20.3",
    "@mdx-js/tag": "^0.20.3",
    "dayjs": "^1.8.11",
    "gatsby": "^2.3.3",
    "gatsby-mdx": "^0.4.4",
    "gatsby-paginate": "^1.0.17",
    "gatsby-plugin-brotli": "^1.0.1",
    "gatsby-plugin-catch-links": "^2.0.13",
    "gatsby-plugin-feed": "^2.1.0",
    "gatsby-plugin-layout": "^1.0.14",
    "gatsby-plugin-manifest": "^2.0.25",
    "gatsby-plugin-netlify": "^2.0.13",
    "gatsby-plugin-no-sourcemaps": "^2.0.2",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-postcss": "^2.0.7",
    "gatsby-plugin-react-helmet": "^3.0.11",
    "gatsby-plugin-sharp": "^2.0.32",
    "gatsby-plugin-sitemap": "^2.0.10",
    "gatsby-remark-autolink-headers": "^2.0.16",
    "gatsby-remark-images": "^3.0.10",
    "gatsby-remark-prismjs": "^3.2.6",
    "gatsby-remark-yuque-images": "^0.2.4",
    "gatsby-source-filesystem": "^2.0.28",
    "gatsby-source-yuque": "^0.0.7",
    "gatsby-transformer-remark": "^2.3.8",
    "lodash": "^4.17.11",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.6.0",
    "prismjs": "^1.16.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0"
  },
  "keywords": [
    "gatsby",
    "blog"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "build:prod": "yarn clean && yarn build",
    "build:server": "netlify-lambda build functions",
    "deploy": "yarn build && ./deploy.sh",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "rm -rf .cache public",
    "format:code": "npm run lint:code -- --fix",
    "lint:code": "eslint --ignore-path .gitignore --ignore-pattern /functions --ext .js,.jsx .",
    "lint:md": "remark src/pages/",
    "format": "prettier --write 'src/**/*.{js,jsx}'"
  },
  "devDependencies": {
    "eslint": "^5.15.3",
    "eslint-plugin-react": "^7.12.4",
    "netlify-lambda": "^1.4.3",
    "prettier": "^1.16.4",
    "remark-cli": "^6.0.1",
    "remark-preset-lint-recommended": "^3.0.2"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended",
      [
        "lint-no-shortcut-reference-link",
        false
      ],
      [
        "lint-no-undefined-references",
        false
      ]
    ]
  }
}
